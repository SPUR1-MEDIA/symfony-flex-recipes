{
    "manifests": {
        "spur1/spur1-api-bundle": {
            "manifest": {
                "bundles": {
                    "Spur1\\BaseBundle\\Spur1BaseBundle": [
                        "all"
                    ]
                },
                "copy-from-package": {
                    "config/packages/spur1_api_bundle.yaml": "%CONFIG_DIR%/packages/spur1_api_bundle.yaml"
                },
                "env": {
                    "BASE_DOMAIN": "'domain.com'"
                },
                "files": {
                    "config/packages/spur1_api_bundle.yaml": {
                        "contents": [
                            "parameters:",
                            "    base_domain: '%env(BASE_DOMAIN)%'",
                            "",
                            "services:",
                            "Spur1\\ApiBundle\\Service\\ApiService:",
                            "    arguments:",
                            "        $base_domain: '%base_domain%'"
                        ],
                        "executable": false
                    }
                }
            },
            "ref": "IOaxD131uEIxwvg1t+KC7UsbhBQyiug+JotYSAHl"
        }
    }
}