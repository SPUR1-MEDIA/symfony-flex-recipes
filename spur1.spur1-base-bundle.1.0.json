
{
    "manifests": {
        "spur1/spur1-base-bundle": {
            "manifest": {
                "bundles": {
                    "Spur1\\BaseBundle\\Spur1BaseBundle": [
                        "all"
                    ]
                },

                "env": {
                    "FEATURE_REGISTER": false,
                    "FEATURE_LOGIN": false,
                    "FEATURE_OFFCANVAS": false
                },

                "copy-from-package": {
                    "assets/controllers/": "assets/controllers/",
                    "config/routes/": "%CONFIG_DIR%/routes/",
                    "config/packages/": "%CONFIG_DIR%/packages/"
                },

                "copy-from-recipe": {
                    "public/": "%PUBLIC_DIR%/",
                    "var/php/sessions/": "%VAR_DIR%/php/sessions/"
                }
            },
            "ref": "KD0CODNEKM7d6g+6fEt4MW2GlIHn2oF2HRH1FadZ"
        }
    }
}